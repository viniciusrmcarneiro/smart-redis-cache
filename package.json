{
  "license": "MIT",
  "private": false,
  "name": "smart-redis-cache",
  "version": "1.0.0",
  "description": "full featured cache solution with complex and recycle cache when data is changed",
  "main": "src/cache",
  "scripts": {
    "semantic-release": "semantic-release",
    "test:only": "mocha --opts ./mocha.opts \"./**/*.spec.js\"",
    "test": "nyc npm run test:only && npm run lint",
    "lint": "eslint .",
    "prettier": "prettier --config ./.prettierrc --write \"{src,test,tests,mocks,examples,example}/**/*.js\"",
    "commit": "npx git-cz"
  },
  "author": "Vinicius Carneiro",
  "contributors": [
    {
      "name": "Vinicius Carneiro",
      "email": "vinicius.ribeiro.carneiro@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/viniciusrmcarneiro/smart-redis-cache.git"
  },
  "keywords": [
    "cache",
    "redis cache",
    "cache manager",
    "cache recycle",
    "recycle",
    "caching",
    "redis",
    "ttl"
  ],
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "nyc": {
    "all": true,
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    },
    "exclude": [
      "**/*.spec.js",
      "src/*.example.js",
      "examples",
      "example",
      "test"
    ],
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary",
      "html"
    ]
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.10",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codacy-coverage": "^3.4.0",
    "codecov.io": "^0.1.6",
    "coveralls": "^3.0.3",
    "decypher": "^0.13.0",
    "eslint": "^5.16.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "glob": "^7.1.3",
    "ioredis": "^4.9.0",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "pg": "^7.9.0",
    "prettier": "^1.16.4",
    "redisgraph.js": "^1.1.4",
    "semantic-release": "^15.13.3",
    "sinon": "^7.3.1"
  },
  "dependencies": {},
  "publishConfig": {
    "access": "public"
  }
}
